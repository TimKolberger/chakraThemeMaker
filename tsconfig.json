{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "ES2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "types": [
      "sketch-types",
      "sketch-internal-types",
      "cocoascript-types",
      "node"
    ]
  },
  "include": ["src", "resources"]
}
